{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["ES6"],
    "moduleResolution": "Node",
    "types": ["lua-types/5.1", "@typescript-to-lua/language-extensions", "@flying-dice/tslua-dcs-gui-types"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "useUnknownInCatchVariables": false
  }
}
